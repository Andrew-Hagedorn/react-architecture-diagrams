{
  "name": "react-architecture-diagrams",
  "version": "0.0.2",
  "description": "Simply, potentially ugly, architecture diagrams rendered as HTML in React",
  "main": "./src/index.js",
  "scripts": {
    "cypress:screenshot": "npx cypress open",
    "cypress:screenshot:docker": "docker run -it -e CYPRESS_baseUrl=http://host.docker.internal:6006/ -e CYPRESS_updateSnapshots=$CYPRESS_updateSnapshots --ipc=host -v $PWD:/e2e -w /e2e cypress/included:6.8.0",
    "storybook": "start-storybook -p 6006"
  },
  "author": "Andrew Hagedorn",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "cypress": "^6.8.0",
    "cypress-image-snapshot": "^4.0.1",
    "prettier": "2.2.1"
  }
}
